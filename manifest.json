{
  "manifest_version": 2,

  "name": "Concise",
  "description": "Replay the composition of your tweet",
  "version": "1.0",

  "permissions": [
    "storage"
  ],

  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": [
        "jquery.js",
        "concise.js"
      ]
    }
  ]
}


